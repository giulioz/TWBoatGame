{
  "name": "SeaDuelServer",
  "version": "1.0.0",
  "description": "SeaDuelServer",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "compile": "node build.js && backpack build",
    "dev": "node build.js && backpack dev | pino -o 'short'",
    "test": "node build.js && mocha test/**/*.ts --exit",
    "db": "mkdir data;mongod --dbpath data",
    "build-angular": "cd ../../angular-app/SeaDuel/ && ng build -c electron --output-path ../../backend/SeaDuelServer/angular_build && cd -",
    "build": "npm run build-angular && npm run compile"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.3.0",
    "luxon": "^1.3.3",
    "mongoose": "^5.2.9",
    "pino": "^4.7.3",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.1.1",
    "socketio-auth": "^0.1.1",
    "source-map-support": "^0.4.15",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "typegoose": "^5.2.1"
  },
  "devDependencies": {
    "@angular/cli": "~6.0.8",
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.0.35",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/luxon": "^1.2.2",
    "@types/mocha": "^2.2.48",
    "@types/mongodb": "^3.1.4",
    "@types/mongoose": "^5.2.5",
    "@types/pino": "^4.7.1",
    "@types/socket.io": "^1.4.36",
    "admin-mongo": "0.0.23",
    "awesome-typescript-loader": "^3.4.1",
    "backpack-core": "^0.4.3",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "mocha": "^4.0.1",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0",
    "swagger-codegen": "^0.1.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.3.2",
    "typescript": "^2.7.2"
  },
  "author": "Giulio Zausa, Alessio Marotta, Francesco Villani"
}
